{
  "name": "env-validation",
  "version": "1.0.0",
  "description": "Environment Variables Validation with Schema",
  "main": "src/validateEnvironment.js",
  "scripts": {
    "start": "node src/validateEnvironment.js",
    "setup": "node scripts/setup.js",
    "updateEnvExample": "node scripts/updateEnvExample.js",
    "generateEnvSchema": "node scripts/generateEnvSchema.js",
    "prestart": "npm run setup && npm run generateEnvSchema && npm run updateEnvExample",
    "dev": "next dev",
    "build": "next build",
    "postinstall": "npm run prestart"
  },
  "keywords": [
    "env",
    "validation",
    "env-validation",
    "schema"
  ],
  "author": "Aravind",
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^10.0.0",
    "valibot": "^1.0.0",
    "next": "^12.0.0"
  },
  "files": [
    "src",
    "scripts",
    ".env.example",
    "envSchema.js",
    "README.md"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/engineeringwitharavind/environment-validation.git"
  },
  "bugs": {
    "url": "https://github.com/engineeringwitharavind/environment-validation/issues"
  },
  "homepage": "https://github.com/engineeringwitharavind/environment-validation#readme"
